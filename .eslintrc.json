{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "window": true,
    "navigator": true,
    "fetch": true,
    "it": true,
    "URL": true,
    "document": true,
    "describe": true,
    "expect": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/click-events-have-key-events": "off",
    "jsx-a11y/no-noninteractive-element-to-interactive-role": "off",
    "arrow-body-style": 0,
    "function-paren-newline": ["error", "multiline"],
    "no-console": 0,
    "react/no-array-index-key": 0
  }
}
